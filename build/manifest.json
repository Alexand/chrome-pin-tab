{
  "name": "Chrome Pin Tab",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Toggle pin tab using ctrl+t",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "commands": {
      "toggle-pintab": {
        "suggested_key": {
          "default": "Alt+P",
          "mac": "MacCtrl+P"
        },
        "description": "Pin Tab"
      }
    }
}
